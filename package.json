{
  "name": "keploy-typescript-quickstart",
  "version": "1.0.0",
  "description": "Keploy TypeScript Quickstart with Express and PostgreSQL",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:push": "prisma db push",
    "keploy:record": "keploy record -c 'npm start'",
    "keploy:test": "keploy test -c 'npm start' --delay 10",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "keploy",
    "typescript",
    "express",
    "postgresql",
    "prisma",
    "testing"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "express": "^4.18.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/cors": "^2.8.17",
    "prisma": "^5.22.0",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2"
  }
}
